{
  "page_heading": {
    "prefix": "/header",
    "body": [
      "#+TITLE:$1",
      "#+TAGS:",
      "-------------------------"
    ],
    "description": "Section Header"
  },
  "checklist_block": {
    "prefix": "/checklist",
    "body": [
      "- [ ] $1",
      "- [ ] ",
      "- [ ] "
    ],
    "description": "Checklist block for subtasks"
  },
  "basic_table_2x2": {
    "prefix": "/table2",
    "body": [
      "| Header 1 | Header 2 |",
      "|----------+----------|",
      "| $1       |          |"
    ],
    "description": "Quick 2x2 org-style table"
  },
  "basic_table_3x3": {
    "prefix": "/table3",
    "body": [
      "| Col A    | Col B    | Col C    |",
      "|----------+----------+----------|",
      "| $1       |          |          |",
      "|          |          |          |",
      "|          |          |          |"
    ],
    "description": "Quick 3x3 org-style table"
  },
  "section_block": {
    "prefix": "/section",
    "body": [
      "* $1",
      "------------------------"
    ],
    "description": "Labeled section block"
  },
  "add_deadline": {
    "prefix": "/dl",
    "body": [
      "DEADLINE: [$1]"
    ],
    "description": "Add deadline to existing task"
  }
}
