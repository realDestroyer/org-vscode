		"configuration": {
			"type": "object",
			"title": "Org-vscode Config",
			"properties": {
				"Org-vscode.headingMarkerStyle": {
					"type": "string",
					"order": 1,
					"default": "unicode",
					"enum": [
						"unicode",
						"asterisks"
					],
					"description": "Controls how headings/tasks are written when changing status/level. 'unicode' uses the extension's unicode symbols (current behavior). 'asterisks' preserves org-mode '*' heading markers for better compatibility with other editors (e.g. Emacs org-mode)."
				},
				"Org-vscode.decorateUnicodeHeadings": {
					"type": "boolean",
					"order": 2,
					"default": false,
					"description": "When enabled (and headingMarkerStyle is 'asterisks'), Org-vscode will render unicode task/day markers via editor decorations without modifying your file content."
				},
				"Org-vscode.decorateHeadingIndentation": {
					"type": "boolean",
					"order": 3,
					"default": true,
					"description": "Visually indent asterisk-mode headings based on asterisk depth (Emacs org-indent-style) using editor decorations without changing file content. This can be enabled independently of unicode marker decorations."
				},
				"Org-vscode.adjustHeadingIndentation": {
					"type": "number",
					"order": 4,
					"default": 2,
					"minimum": 0,
					"multipleOf": 1,
					"description": "Spaces per level used when toggling heading symbols with Alt+Left/Alt+Right (0 disables). Also used by decorated heading indentation when enabled."
				},
				"Org-vscode.autoIndentNonHeaderText": {
					"type": "boolean",
					"order": 4.1,
					"default": false,
					"description": "When enabled, pressing Enter after a heading will auto-indent the new (non-heading) line to match the heading's indentation plus bodyIndentation spaces."
				},
				"Org-vscode.bodyIndentation": {
					"type": "number",
					"order": 4.2,
					"default": 2,
					"minimum": 0,
					"multipleOf": 1,
					"description": "Number of spaces to indent body/planning lines under a heading (used by autoIndentNonHeaderText and planning-line writers). Default is 2."
				},
				"Org-vscode.logIntoDrawer": {
					"type": "boolean",
					"order": 4.3,
					"default": false,
					"description": "When enabled, Org-vscode records completion events as Org-mode style log entries inside a drawer (default: LOGBOOK)."
				},
				"Org-vscode.logDrawerName": {
					"type": "string",
					"order": 4.4,
					"default": "LOGBOOK",
					"description": "Drawer name to use when logIntoDrawer is enabled (Org-mode convention is LOGBOOK)."
				},
				"Org-vscode.folderPath": {
					"type": "string",
					"order": 6,
					"default": "",
					"description": "Main folder"
				},
				"Org-vscode.dateFormat": {
					"type": "string",
					"order": 5,
					"default": "YYYY-MM-DD",
					"enum": [
						"MM-DD-YYYY",
						"DD-MM-YYYY",
						"YYYY-MM-DD"
					],
					"description": "org file date format (YYYY-MM-DD is the Emacs Org-mode standard)"
				},
				"Org-vscode.includeContinuedInTaggedAgenda": {
					"type": "boolean",
					"order": 7,
					"default": false,
					"description": "When enabled, Tagged Agenda results will include CONTINUED tasks. By default they are omitted since CONTINUED is intended as a historical breadcrumb when a task moved forward."
				},
				"Org-vscode.alignSchedulesAlignTags": {
					"type": "boolean",
					"order": 7.01,
					"default": true,
					"description": "When enabled, the Align Schedules command will align end-of-line tags (e.g. :WORK:URGENT:) on heading/task lines only (never on drawers like :PROPERTIES:)."
				},
				"Org-vscode.alignSchedulesAlignInlineScheduled": {
					"type": "boolean",
					"order": 7.02,
					"default": true,
					"description": "When enabled, the Align Schedules command will align legacy inline SCHEDULED: stamps that appear on the same line as a task heading."
				},
				"Org-vscode.alignSchedulesNormalizePlanningLines": {
					"type": "boolean",
					"order": 7.03,
					"default": true,
					"description": "When enabled, the Align Schedules command will normalize the immediate planning line under a task (SCHEDULED/DEADLINE/CLOSED) using the configured bodyIndentation."
				},
				"Org-vscode.workflowStates": {
					"scope": "window",
					"type": "array",
					"order": 7.1,
					"default": [
						{
							"keyword": "TODO",
							"marker": "⊙",
							"isDoneLike": false,
							"stampsClosed": false,
							"triggersForward": false,
							"agendaVisibility": "show",
							"taggedAgendaVisibility": "show"
						},
						{
							"keyword": "IN_PROGRESS",
							"marker": "⊘",
							"isDoneLike": false,
							"stampsClosed": false,
							"triggersForward": false,
							"agendaVisibility": "show",
							"taggedAgendaVisibility": "show"
						},
						{
							"keyword": "CONTINUED",
							"marker": "⊜",
							"isDoneLike": false,
							"stampsClosed": false,
							"triggersForward": true,
							"agendaVisibility": "hide",
							"taggedAgendaVisibility": "hide"
						},
						{
							"keyword": "DONE",
							"marker": "⊖",
							"isDoneLike": true,
							"stampsClosed": true,
							"triggersForward": false,
							"agendaVisibility": "hide",
							"taggedAgendaVisibility": "hide"
						},
						{
							"keyword": "ABANDONED",
							"marker": "⊗",
							"isDoneLike": true,
							"stampsClosed": false,
							"triggersForward": false,
							"agendaVisibility": "hide",
							"taggedAgendaVisibility": "hide"
						}
					],
					"items": {
						"type": "object",
						"required": ["keyword"],
						"properties": {
							"keyword": {
								"type": "string",
								"description": "Task state keyword (e.g. TODO, IN_PROGRESS)."
							},
							"marker": {
								"type": "string",
								"description": "Optional leading marker (unicode symbol) that can prefix headings in unicode mode."
							},
							"isDoneLike": {
								"type": "boolean",
								"default": false,
								"description": "Whether this state is considered completed for exports/reports."
							},
							"stampsClosed": {
								"type": "boolean",
								"default": false,
								"description": "When switching into this state, add a CLOSED: timestamp (if supported by the command/view)."
							},
							"triggersForward": {
								"type": "boolean",
								"default": false,
								"description": "Treat this as the forward/carryover state (replaces hard-coded CONTINUED)."
							},
							"agendaVisibility": {
								"type": "string",
								"enum": ["show", "hide"],
								"default": "show",
								"description": "Whether tasks in this state appear in Agenda View by default."
							},
							"taggedAgendaVisibility": {
								"type": "string",
								"enum": ["show", "hide"],
								"default": "show",
								"description": "Whether tasks in this state appear in Tagged Agenda by default."
							}
						}
					},
					"description": "Ordered list of workflow states. Leave unset to use the extension defaults."
				},
				"Org-vscode.decorateTodoStateLines": {
					"type": "boolean",
					"order": 8,
					"default": true,
					"description": "Decorate entire task heading lines with a background color based on TODO keyword (uses your editor.tokenColorCustomizations TextMate rule background for the keyword scopes)."
				},
				"Org-vscode.autoDoneWhenAllCheckboxesChecked": {
					"type": "boolean",
					"order": 9,
					"default": false,
					"description": "When enabled: automatically change a task heading to DONE when all checkboxes under it are checked, and change it back to IN_PROGRESS if any checkbox becomes unchecked. Works for editor edits and edits written by the agenda view."
				},
				"Org-vscode.decorateEmphasis": {
					"type": "boolean",
					"order": 10,
					"default": true,
					"description": "Render Org emphasis markers (*bold*, /italic/, _underline_) using editor decorations and hide the marker characters unless the cursor is on them."
				},
				"Org-vscode.decorateMath": {
					"type": "boolean",
					"order": 10.5,
					"default": true,
					"description": "Render common LaTeX commands (e.g. \\alpha, \\sum) inside $...$ and $$...$$ math using editor decorations."
				},
				"Org-vscode.strictActiveTimestamps": {
					"type": "boolean",
					"order": 11,
					"default": false,
					"description": "When enabled, only active timestamps <YYYY-MM-DD...> appear in agenda and calendar views (standard Org behavior where inactive [...] timestamps are reference-only). When disabled (default), both <...> and [...] timestamps appear in agenda views for backward compatibility with older org-vscode files."
				}
			}
		},
